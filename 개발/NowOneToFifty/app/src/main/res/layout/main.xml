<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/java_root_view"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical"
    android:background="#000000">
	<LinearLayout
		android:orientation="vertical" android:layout_width="fill_parent" android:layout_marginBottom="60dip"
		android:layout_height="fill_parent" android:background="@drawable/start">
	<LinearLayout android:id="@+id/LinearLayout01"
		android:layout_height="wrap_content" android:layout_width="fill_parent"
		android:orientation="vertical">
		 <LinearLayout android:layout_width="fill_parent"
		    android:layout_height="wrap_content"
		    android:orientation="horizontal"
			android:gravity="center"
			android:layout_marginTop="5dip"
			android:layout_marginBottom="5dip">
		    <ImageView android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:src="@drawable/f00"
		    android:id="@+id/mi1"/>
		    <ImageView android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:src="@drawable/f10"
		    android:id="@+id/div2"/>
		    <ImageView android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:src="@drawable/f00"
		    android:id="@+id/s2"/>
		    <ImageView android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:src="@drawable/f00"
		    android:id="@+id/s1"/>
		    <ImageView android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:src="@drawable/f11"
		    android:id="@+id/div3"/>
		    <ImageView android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:src="@drawable/f00"
		    android:id="@+id/m2"/>
		</LinearLayout>
		<LinearLayout android:id="@+id/LinearLayout02"
			android:layout_height="wrap_content" android:layout_width="fill_parent">
			<Button android:id="@+id/Button26" android:layout_width="fill_parent" 
			    android:layout_marginLeft="5dip" android:layout_marginRight="5dip"
				android:text="시작" android:layout_weight="1" android:layout_height="fill_parent"
				android:textSize="12pt" android:background="@drawable/bt1"></Button>
			<Button android:id="@+id/Button27" android:layout_weight="1"
				android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_marginRight="5dip"
				android:text="현재순위" android:textSize="12pt" android:background="@drawable/bt2"></Button>
		</LinearLayout>
	</LinearLayout>
	<TableLayout android:id="@+id/TableLayout01"
		android:layout_width="fill_parent" android:layout_height="wrap_content"
		android:layout_weight="1">
		<TableRow android:id="@+id/TableRow01" android:layout_width="wrap_content"
			android:paddingTop="3px" android:paddingBottom="3px"
			android:layout_height="wrap_content" android:layout_weight="1">
			<Button android:id="@+id/Button01" android:layout_width="fill_parent"
				android:layout_weight="1" android:background="@drawable/im1"
				android:layout_marginLeft="3px" android:layout_height="match_parent"></Button>
			<Button android:id="@+id/Button02" android:layout_width="fill_parent"
				android:layout_weight="1" android:background="@drawable/im2"
				android:layout_marginLeft="3px" android:layout_height="match_parent"></Button>
			<Button android:id="@+id/Button03" android:layout_width="fill_parent"
				android:layout_weight="1" android:background="@drawable/im3"
				android:layout_marginLeft="3px" android:layout_height="match_parent"></Button>
			<Button android:id="@+id/Button04" android:layout_width="fill_parent"
				android:layout_weight="1" android:background="@drawable/im4"
				android:layout_marginLeft="3px" android:layout_height="match_parent"></Button>
			<Button android:id="@+id/Button05" android:layout_width="fill_parent"
				android:layout_weight="1" android:background="@drawable/im5"
				android:layout_marginLeft="3px" android:layout_marginRight="3px"
				android:layout_height="match_parent"></Button>

		</TableRow>
		<TableRow android:id="@+id/TableRow02" android:layout_height="wrap_content"
			android:layout_width="fill_parent" android:paddingBottom="3px"
			android:layout_weight="1">

			<Button android:id="@+id/Button06" android:layout_width="fill_parent"
				android:layout_weight="1" android:background="@drawable/im6"
				android:layout_marginLeft="3px" android:layout_height="match_parent"></Button>
			<Button android:id="@+id/Button07" android:layout_width="fill_parent"
				android:layout_weight="1" android:background="@drawable/im7"
				android:layout_marginLeft="3px" android:layout_height="match_parent"></Button>
			<Button android:id="@+id/Button08" android:layout_width="fill_parent"
				android:layout_weight="1" android:background="@drawable/im8"
				android:layout_marginLeft="3px" android:layout_height="match_parent"></Button>
			<Button android:id="@+id/Button09" android:layout_width="fill_parent"
				android:layout_weight="1" android:background="@drawable/im9"
				android:layout_marginLeft="3px" android:layout_height="match_parent"></Button>
			<Button android:id="@+id/Button10" android:layout_width="fill_parent"
				android:layout_weight="1" android:background="@drawable/im10"
				android:layout_marginLeft="3px" android:layout_marginRight="3px"
				android:layout_height="match_parent"></Button>
		</TableRow>
		<TableRow android:id="@+id/TableRow03" android:layout_height="wrap_content"
			android:layout_width="fill_parent" android:paddingBottom="3px"
			android:layout_weight="1">

			<Button android:id="@+id/Button11" android:layout_width="fill_parent"
				android:layout_weight="1" android:background="@drawable/im11"
				android:layout_marginLeft="3px" android:layout_height="match_parent"></Button>
			<Button android:id="@+id/Button12" android:layout_width="fill_parent"
				android:layout_weight="1" android:background="@drawable/im12"
				android:layout_marginLeft="3px" android:layout_height="match_parent"></Button>
			<Button android:id="@+id/Button13" android:layout_width="fill_parent"
				android:layout_weight="1" android:background="@drawable/im13"
				android:layout_marginLeft="3px" android:layout_height="match_parent"></Button>
			<Button android:id="@+id/Button14" android:layout_width="fill_parent"
				android:layout_weight="1" android:background="@drawable/im1"
				android:layout_marginLeft="3px" android:layout_height="match_parent"></Button>
			<Button android:id="@+id/Button15" android:layout_width="fill_parent"
				android:layout_weight="1" android:background="@drawable/im2"
				android:layout_marginLeft="3px" android:layout_marginRight="3px"
				android:layout_height="match_parent"></Button>
		</TableRow>
		<TableRow android:id="@+id/TableRow04" android:layout_height="wrap_content"
			android:layout_width="fill_parent" android:paddingBottom="3px"
			android:layout_weight="1">


			<Button android:id="@+id/Button16" android:layout_width="fill_parent"
				android:layout_weight="1" android:background="@drawable/im3"
				android:layout_marginLeft="3px" android:layout_height="match_parent"></Button>
			<Button android:id="@+id/Button17" android:layout_width="fill_parent"
				android:layout_weight="1" android:background="@drawable/im4"
				android:layout_marginLeft="3px" android:layout_height="match_parent"></Button>
			<Button android:id="@+id/Button18" android:layout_width="fill_parent"
				android:layout_weight="1" android:background="@drawable/im5"
				android:layout_marginLeft="3px" android:layout_height="match_parent"></Button>
			<Button android:id="@+id/Button19" android:layout_width="fill_parent"
				android:layout_weight="1" android:background="@drawable/im6"
				android:layout_marginLeft="3px" android:layout_height="match_parent"></Button>
			<Button android:id="@+id/Button20" android:layout_width="fill_parent"
				android:layout_weight="1" android:background="@drawable/im7"
				android:layout_marginLeft="3px" android:layout_marginRight="3px"
				android:layout_height="match_parent"></Button>
		</TableRow>
		<TableRow android:id="@+id/TableRow05" android:layout_height="wrap_content"
			android:layout_width="fill_parent" android:paddingBottom="3px"
			android:layout_weight="1">
			<Button android:id="@+id/Button21" android:layout_width="fill_parent"
				android:layout_weight="1" android:background="@drawable/im8"
				android:layout_marginLeft="3px" android:layout_height="match_parent"></Button>
			<Button android:id="@+id/Button22" android:layout_width="fill_parent"
				android:layout_weight="1" android:background="@drawable/im9"
				android:layout_marginLeft="3px" android:layout_height="match_parent"></Button>
			<Button android:id="@+id/Button23" android:layout_width="fill_parent"
				android:layout_weight="1" android:background="@drawable/im10"
				android:layout_marginLeft="3px" android:layout_height="match_parent"></Button>
			<Button android:id="@+id/Button24" android:layout_width="fill_parent"
				android:layout_weight="1" android:background="@drawable/im11"
				android:layout_marginLeft="3px" android:layout_height="match_parent"></Button>
			<Button android:id="@+id/Button25" android:layout_width="fill_parent"
				android:layout_weight="1" android:background="@drawable/im12"
				android:layout_marginLeft="3px" android:layout_marginRight="3px"
				android:layout_height="match_parent"></Button>
		</TableRow>

	</TableLayout>
</LinearLayout>
</RelativeLayout>